
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>Fast Service a Home Service Category Bootstrap Responsive Website Template | Gallery :: w3layouts</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta charset="utf-8">
    <meta name="keywords" content="Fast Service a Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template,
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <link href="~/Content/Homesripts/css/bootstrap.min.css" rel="stylesheet" type="text/css" media="all">
    <link href="~/Content/Homesripts/css/fontawesome-all.min.css" rel="stylesheet" type="text/css" media="all">
    <link href="~/Content/Homesripts/css/prettyPhoto.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" type="text/css" href="~/Content/Homesripts/css/style_common.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/Homesripts/css/style1.css" />
    <link href="~/Content/Homesripts/css/style.css" rel="stylesheet" type="text/css" media="all" />
    <link href="//fonts.googleapis.com/css?family=Roboto:400,500,700,900" rel="stylesheet">
    <link href="//fonts.googleapis.com/css?family=Open+Sans:400,600,700,800" rel="stylesheet">
</head>
<body>
    <header>
        <!-- header -->
        <div class="header-w3l">

            <nav class="navbar navbar-expand-lg navbar-light">
                @Html.ActionLink("MBGIE Pagent", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item ">
                            @Html.ActionLink("Home", "Index", "Home", new { area = "" }, new { @class = "nav-link" })
                        </li>
                        <li class="nav-item">
                            @Html.ActionLink("About", "About", "Home", new { area = "" }, new { @class = "nav-link" })
                        </li>
                        <li class="nav-item">
                            @Html.ActionLink("Terms", "Terms", "Home", new { area = "" }, new { @class = "nav-link" })
                        </li>
                        <li class="nav-item dropdown">
                            @Html.ActionLink("Gallary", "Gallary", "Home", new { area = "" }, new { @class = "nav-link" })
                        </li>
                        <li class="nav-item">
                            @Html.ActionLink("Contact Us", "Contact", "Home", new { area = "" }, new { @class = "nav-link" })
                        </li>
                        <li class="nav-item">
                            @Html.ActionLink("Contestants", "Contestants", "Home", new { area = "" }, new { @class = "nav-link" })
                        </li>

                        @using Microsoft.AspNet.Identity
                        @if (Request.IsAuthenticated)
                        {
                            using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm", @class = "navbar-right" }))
                            {
                                @Html.AntiForgeryToken()

                                <li>
                                    @Html.ActionLink("Hello " + User.Identity.GetUserName() + "!", "Index", "Manage", routeValues: null, htmlAttributes: new { title = "Manage", @class = "nav-link" })
                                </li>
                                <li class="w3"><a href="javascript:document.getElementById('logoutForm').submit()">Log Out</a></li>

                            }
                        }
                        else
                        {
                            <li class="w3">@Html.ActionLink("Register", "Register", "Account", routeValues: null, htmlAttributes: new { @class = "nav-link" })</li>
                            <li class="w3">@Html.ActionLink("Log in", "Login", "Account", routeValues: null, htmlAttributes: new { @class = "nav-link" })</li>

                        }

                    </ul>

            </nav>

            <!-- //header -->
            <!-- banner -->
            <!-- banner-slider -->
            <div class="w3l_banner_info">
                <div class="slider1">
                    <div class="callbacks_container">
                        <ul class="rslides" id="slider3">
                            <li>
                                <div class="slider1-img">
                                    <div class="slider_banner_info">


                                    </div>
                                </div>
                            </li>


                        </ul>
                    </div>
                </div>
            </div>
            <!-- //banner-slider -->
        </div>
    </header>

    @RenderBody()
    <!-- Footer-->
    <section class="footer">
        <div class="container">
            <h3>MBGIE</h3>
            <div class="wrapper">
                <ul class="social-icons icon-circle icon-zoom list-unstyled list-inline">
                    <li> <a href="#"><i class="fab fa-facebook-f"></i></a></li>
                    <li> <a href="#"><i class="fab fa-twitter"></i></a></li>
                    <li> <a href="#"><i class="fab fa-linkedin-in"></i></a></li>
                </ul>
            </div>
            <div class="copyright">
                <p>© 2018 MBGIE. All Rights Reserved | Design by <a href="https://w3layouts.com/">W3layouts</a></p>

            </div>
        </div>
    </section>
    <!-- /Footer-->
    <script src="~/Content/Homesripts/js/jquery.min.v3.js"></script>
    <script src="~/Content/Homesripts/js/bootstrap.min.js"></script>
    <script src="~/Content/Homesripts/js/move-top.js"></script>
    <script src="~/Content/Homesripts/js/easing.js"></script>
    <script src="~/Content/Homesripts/js/SmoothScroll.min.js"></script>
    <script src="~/Content/Homesripts/js/easy-responsive-tabs.js"></script>


</body>
</html>
